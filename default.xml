<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- tsinghua aosp mirror -->
  <remote  name="aosp"
           fetch="https://aosp.tuna.tsinghua.edu.cn" />

  <!-- official google aosp repository -->
  <!--
  <remote  name="aosp"
           fetch="https://android.googlesource.com" />
  -->

  <!-- lampoo repository -->
  <remote  name="lampoo"
           fetch="https://github.com/Lampoo" />

  <default revision="refs/tags/android-4.4.4_r2.0.1"
           remote="aosp"
           sync-j="4" />

  <!-- at91bootstrap and kernel sources for atmel sama5d2 device -->
  <project path="at91bootstrap" name="at91bootstrap" remote="lampoo" revision="master" groups="pdk" />
  <project path="kernel" name="linux-at91" remote="lampoo" revision="master" groups="pdk" />

  <!-- update tools for atmel sama5 device -->
  <project path="external/sam-ba-3.2.2" name="sam-ba-3.2.2" remote="lampoo" revision="master" groups="pdk" />

  <!-- bootstrap source -->
  <project path="vendor/lampoo" name="vendor-lampoo" remote="lampoo" revision="master" groups="pdk" >
    <copyfile src="bootstrap/Makefile" dest="Makefile" />
  </project>

  <!-- aosp native sources -->
  <project path="build" name="platform/build" groups="pdk" />
  <project path="abi/cpp" name="platform/abi/cpp" groups="pdk" />
  <project path="bionic" name="platform/bionic" groups="pdk" />
  <project path="external/compiler-rt" name="platform/external/compiler-rt" groups="pdk" />
  <project path="external/e2fsprogs" name="platform/external/e2fsprogs" groups="pdk" />
  <project path="external/gcc-demangle" name="platform/external/gcc-demangle" groups="pdk" />
  <project path="external/lzma" name="platform/external/lzma" />
  <project path="external/mksh" name="platform/external/mksh" groups="pdk" />
  <project path="external/openssl" name="platform/external/openssl" groups="pdk" />
  <project path="external/stlport" name="platform/external/stlport" groups="pdk" />
  <project path="external/zlib" name="platform/external/zlib" groups="pdk" />
  <project path="frameworks/native" name="platform/frameworks/native" groups="pdk" />
  <project path="hardware/libhardware" name="platform/hardware/libhardware" groups="pdk" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" name="platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" groups="pdk,linux,arm" revision="refs/tags/android-6.0.0_r1" />
  <project path="prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.15-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.15-4.8"  groups="pdk,linux" revision="refs/tags/android-6.0.0_r1" />
  <project path="system/core" name="platform/system/core" groups="pdk" />
  <project path="system/extras" name="platform/system/extras" groups="pdk" />
</manifest>
